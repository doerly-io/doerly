    <data name="ResetPassword" xml:space="preserve">
        <value>&lt;!DOCTYPE html&gt;
&lt;html lang="en"&gt;

&lt;head&gt;
    &lt;meta charset="UTF-8"&gt;
    &lt;meta name="viewport" content="width=device-width, initial-scale=1"&gt;
    &lt;meta http-equiv="X-UA-Compatible" content="IE=Edge"&gt;
    &lt;title&gt;Email Template&lt;/title&gt;
    &lt;style&gt;
        body {
            font-family: Arial, Helvetica, sans-serif;
            font-size: 14px;
            color: #000;
            background-color: #FFF;
            margin: 0;
            padding: 0;
        }

        a {
            color: #2563eb;
            text-decoration: none;
        }

        .wrapper {
            width: 100%;
            table-layout: fixed;
        }

        .container {
            width: 100%;
            max-width: 600px;
            margin: 0 auto;
            background-color: #FFF;
        }

        .header {
            background-color: #10b981;
            padding: 30px 20px 22px;
            text-align: left;
            color: #FFF;
            font-size: 24px;
            font-weight: bold;
        }
        

        .content {
            padding: 18px 15px;
            line-height: 22px;
            text-align: left;
        }

        .footer {
            background-color: #10b981;
            padding: 30px 20px 22px;
            text-align: left;
            color: #FFF;
            font-size: 10px;
            font-weight: bold;
        }

        @media screen and (max-width: 480px) {
            .container {
                width: 100% !important;
            }

            .content {
                padding: 10px;
            }
        }
    &lt;/style&gt;
&lt;/head&gt;

&lt;body&gt;
    &lt;div class="wrapper"&gt;
        &lt;table class="container"&gt;
            &lt;tr&gt;
                &lt;td class="header"&gt;DOERLY&lt;/td&gt;
            &lt;/tr&gt;
            &lt;tr&gt;
                &lt;td class="content"&gt;
                    {{welcomeVerificationText}} &lt;a href="{{verificationLink}}"&gt;{{verificationLinkText}}&lt;/a&gt;
                &lt;/td&gt;
            &lt;/tr&gt;
            &lt;tr&gt;
                &lt;td class="content"&gt;
                    {{bestRegardsText}}
                &lt;/td&gt;
            &lt;/tr&gt;
            &lt;tr&gt;
                &lt;td class="footer"&gt;&lt;/td&gt;
            &lt;/tr&gt;
        &lt;/table&gt;
    &lt;/div&gt;
&lt;/body&gt;

&lt;/html&gt;</value>
    </data>
